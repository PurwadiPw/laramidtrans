{
    "name": "pw/laramidtrans",
    "description": "A Midtrans Payment Wrapper for Laravel",
    "keywords": [
        "laravel", "payment", "midtrans", "veritrans"
    ],
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Purwadi",
            "email": "purwadie97@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "5.1.x|5.2.x|5.3.x|5.4.x|5.5.x|5.6.x|5.7.x",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0|~5.0",
        "vlucas/phpdotenv": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Pw\\Midtrans\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pw\\Midtrans\\MidtransServiceProvider"
            ],
            "aliases": {
                "Midtrans": "Pw\\Midtrans\\Facades\\Midtrans"
            }
        }
    },
    "config":{
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
